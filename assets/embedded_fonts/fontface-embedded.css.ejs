.font-embedded-0 { font-family: 'qzone'; }
.font-embedded-1 { font-family: 'entypo'; }
.font-embedded-2 { font-family: 'fontawesome'; }
.font-embedded-3 { font-family: 'iconic'; }
.font-embedded-4 { font-family: 'typicons'; }
.font-embedded-5 { font-family: 'modernpics'; }
.font-embedded-6 { font-family: 'websymbols'; }

@font-face {
  font-family: 'qzone';
  src: url('<%= asset_path("embedded_fonts/qzone.eot") %>');
  src: url('<%= asset_path("embedded_fonts/qzone.eot") %>?#iefix') format('embedded-opentype'),
       url('<%= asset_path("embedded_fonts/qzone.woff") %>') format('woff'),
       url('<%= asset_path("embedded_fonts/qzone.ttf") %>') format('truetype'),
       url('<%= asset_path("embedded_fonts/qzone.svg") %>#qzone') format('svg');
  font-weight: normal;
  font-style: normal;
}

